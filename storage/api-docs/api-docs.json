            "order": {
                "required": [
                    "id,title"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "dealer_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "6"
                    },
                    "client_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "5"
                    },
                    "state_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "title": {
                        "description": "Date expected for the package to arrive",
                        "type": "date",
                        "readOnly": "true",
                        "example": "2000-04-12"
                    },
                    "order": {
                        "description": "Order that determines wich order will be delivered first",
                        "type": "integer",
                        "readOnly": "true",
                        "example": "6"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "order"
                }
            },
            "orderline": {
                "required": [
                    "id,product_id,quantity,order_id"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "product_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "Iphone 11"
                    },
                    "order_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "quantity": {
                        "type": "integer",
                        "readOnly": "true",
                        "example": "5"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "orderline"
                }
            },
            "provider": {
                "required": [
                    "id,name,email,phone"
