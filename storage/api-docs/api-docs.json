{
    "openapi": "3.0.0",
    "info": {
        "title": "Batoilogic G01",
        "version": "1.0.0"
    },
    "paths": {
        "/order": {
            "post": {
                "tags": [
                    "orders"
                ],
                "summary": "Add an order",
                "description": "Store a new order in the database",
                "operationId": "postOrder",
                "requestBody": {
                    "description": "Posting an order",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "dealer_id",
                                    "client_id",
                                    "state_id",
                                    "delivery_date",
                                    "order"
                                ],
                                "properties": {
                                    "dealer_id": {
                                        "type": "integer",
                                        "example": "6"
                                    },
                                    "client_id": {
                                        "type": "integer",
                                        "example": "15"
                                    },
                                    "state_id": {
                                        "type": "integer",
                                        "example": "1"
                                    },
                                    "delivery_date": {
                                        "type": "Date",
                                        "example": "2000-04-12"
                                    },
                                    "order": {
                                        "type": "integer",
                                        "example": "5"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Success"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "order": {
                "required": [
                    "id,title"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "dealer_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "6"
                    },
                    "client_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "5"
                    },
                    "state_id": {
                        "type": "string",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "title": {
                        "description": "Date expected for the package to arrive",
                        "type": "date",
                        "readOnly": "true",
                        "example": "2000-04-12"
                    },
                    "order": {
                        "description": "Order that determines wich order will be delivered first",
                        "type": "integer",
                        "readOnly": "true",
                        "example": "6"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "order"
                }
            }
        },
        "securitySchemes": {
            "apiAuth": {
                "type": "http",
                "description": "Login with email and password to get the authentication",
                "name": "Token based Based",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "security": [
        []
    ]
}